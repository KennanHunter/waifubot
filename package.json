  "dependencies": {
    "slash-create": "^5.6.1"
  },
  "devDependencies": {
    "@types/node": "^16.7.13",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vercel/node": "^1.12.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "prettier": "^2.8.1",
    "slash-up": "^1.1.2",
    "typescript": "^4.4.2",
    "husky": "^8.0.0"
  }
}
