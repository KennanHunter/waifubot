{
	"name": "waifu-bot",
	"version": "1.0.0",
	"description": "A template for slash-create in Vercel",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "tsc && vite build",
		"dev": "vite",
		"format": "prettier -w .",
		"lint": "npx eslint --ext .ts .",
		"lint:fix": "npx eslint --ext .ts . --fix",
		"prepare": "husky install",
		"preview": "vite preview",
		"sync": "slash-up sync",
		"sync:dev": "slash-up sync -e development"
	},
	"lint-staged": {
		"*.ts": "prettier -w"
	},
	"dependencies": {
		"slash-create": "^5.6.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9",
		"@vitejs/plugin-react": "^3.0.0",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"@types/node": "^18.11.17",
		"@typescript-eslint/eslint-plugin": "^5.47.0",
		"@typescript-eslint/parser": "^5.47.0",
		"@vercel/node": "^2.8.2",
		"husky": "^8.0.0",
		"lint-staged": "^13.1.0",
		"prettier": "^2.8.1",
		"slash-up": "^1.1.2"
	}
}
