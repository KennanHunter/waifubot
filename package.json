{
  "name": "waifu-bot",
  "version": "1.0.0",
  "description": "A template for slash-create in Vercel",
  "private": true,
  "scripts": {
    "build": "tsc",
    "format": "prettier -w .",
    "lint": "npx eslint --ext .ts .",
    "lint:fix": "npx eslint --ext .ts . --fix",
    "prepare": "husky install",
    "sync": "slash-up sync",
    "sync:dev": "slash-up sync -e development"
  },
  "lint-staged": {
    "*.ts": "prettier -w"
  },
  "dependencies": {
    "slash-create": "^5.6.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vercel/node": "^2.8.2",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "slash-up": "^1.1.2",
    "typescript": "^4.4.2"
  }
}
