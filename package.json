    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint": "^8.30.0",
    "husky": "^8.0.0",
    "prettier": "^2.8.1",
    "slash-up": "^1.1.2",
    "typescript": "^4.4.2"
